{
    "name" : "Emojikey",
    "manifest_version" : 2,
    "version" : "0.2.1",
    "description" : "Search for and type emojis",
    "icons" : { "256" : "heart-eyes.png" },
    "background": {
        "scripts": [
            "jquery-3.0.0.min.js",
            "search.js",
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": [
                "styles.css"
            ],
            "js": [
                "jquery-3.0.0.min.js",
                "jquery-textrange.js",
                "content.js"
            ]
        }
   ],
   "content_security_policy": "object-src 'self'; script-src 'self' 'sha256-3uVIOUwpoPA7BJJUdaKXxOSGN+EgMLRnYEeSibRayN4=';"
}
